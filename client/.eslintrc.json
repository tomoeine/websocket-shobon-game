{
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": "plugin:vue/recommended",
  "globals": {
  },
  "rules": {
    "no-console": "off",
    "no-extra-semi": "warn",
    "no-undef": "warn",
    "quotes": [
      "warn",
      "single"
    ],
    "space-before-blocks": [
      "warn",
      {
        "functions": "always"
      }
    ]
  }
}